    "uuid-random": "^1.3.2",
    "vue-moment": "^4.1.0"
  },
  "devDependencies": {
    "@quasar/app": "^2.0.0",
    "devtron": "^1.4.0",
    "electron": "^9.0.0",
    "electron-debug": "^3.0.1",
    "electron-devtools-installer": "^3.0.0"
  },
