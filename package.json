{
  "name": "pump-controller",
  "version": "0.0.1",
  "description": "Application to control and monitor pumps",
  "productName": "Pump Controller",
  "author": "Andre D (dredix84@gmail.com)",
  "private": true,
  "scripts": {
    "dev": "quasar dev -p 8081",
    "dev-electron": "quasar dev -m electron -p 8081",
    "dev-android": "quasar dev -m cordova -T android -p 8081",
    "build-android": "quasar build -m cordova -T android && npm run cordova-build-android",
    "cordova-build-android": "cd src-cordova && cordova build android"
  },
